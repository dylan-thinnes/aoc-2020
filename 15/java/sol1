#!/usr/bin/java --source 14
import java.util.HashMap;
import java.util.Scanner;

class Sol1 {
    public static class History {
        class Entry {
            protected Integer key;
            protected boolean firstTime;
            private int last1;
            private int last2;

            Entry (Integer key, int last1) {
                this.key = key;
                this.last1 = last1;
                this.last2 = 0;
                this.firstTime = true;
            }

            public Entry touch (int last1) {
                this.last2 = this.last1;
                this.last1 = last1;
                this.firstTime = false;
                return this;
            }

            public int diff () {
                return this.last1 - this.last2;
            }
        }

        private HashMap<Integer, Entry> history;
        protected int step;
        protected Entry lastInt;

        History () {
            this.history = new HashMap();
            this.step = 0;
            this.lastInt = null;
        }

        public void insert (int newInt) {
            this.save(newInt);
        }

        public void next () {
            if (this.lastInt.firstTime) {
                this.save(0);
            } else {
                this.save(this.lastInt.diff());
            }
        }

        public void save (int val) {
            Entry entry = this.history.get(val);

            if (entry == null) {
                entry = new Entry(val, this.step);
                this.history.put(val, entry);
            } else {
                entry.touch(this.step);
            }

            this.lastInt = entry;
            this.step++;
        }
    }

    public static void main (String [] args) {
        Scanner scanner = new Scanner(System.in).useDelimiter(",|$");
        History history = new History();

        try {
            while (true) {
                history.insert(scanner.nextInt());
            }
        } catch (java.util.InputMismatchException e) {
        }

        while (history.step < 2020) {
            history.next();
        }

        System.out.println(history.lastInt.key);
    }
}
